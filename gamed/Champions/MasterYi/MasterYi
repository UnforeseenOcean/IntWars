#ifndef _CHAMPION_MASTERYI_H
#define _CHAMPION_MASTERYI_H

#include "Champion.h"

class MasterYi : public Champion {

public:
   MasterYi(Map* map, uint32 id) : Champion("MasterYi", map, id) {
      stats->setCurrentHealth(536.f);
      stats->setMaxHealth(536.f);
      stats->setCurrentMana(222.f);
      stats->setMaxMana(222.f);
      stats->setBaseAd(58.f);
      stats->setRange(125.f);
      stats->setMovementSpeed(355.f);
      stats->setArmor(22.f);
      stats->setMagicArmor(31.25f);
      stats->setHp5(7.15f);
      stats->setMp5(6.95f);
	    stats->setBaseSpeed(0.679f)
   }


};

#endif
