#ifndef _CHAMPION_SKARNER_H
#define _CHAMPION_SKARNER_H

#include "Champion.h"

class Skarner : public Champion {

public:
   Skarner(Map* map, uint32 id) : Champion("Skarner", map, id) {
      stats->setCurrentHealth(536.f);
      stats->setMaxHealth(536.f);
      stats->setCurrentMana(245.f);
      stats->setMaxMana(245.f);
      stats->setBaseAd(58.3f);
      stats->setRange(125.f);
      stats->setMovementSpeed(345.f);
      stats->setArmor(26.8f);
      stats->setMagicArmor(31.25f);
      stats->setHp5(8.35f);
      stats->setMp5(6.9f);
	    stats->setBaseSpeed(0.625f)
   }


};

#endif
