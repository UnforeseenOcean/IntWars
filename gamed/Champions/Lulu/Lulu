#ifndef _CHAMPION_LULU_H
#define _CHAMPION_LULU_H

#include "Champion.h"

class Lulu : public Champion {

public:
   Lulu(Map* map, uint32 id) : Champion("Lulu", map, id) {
      stats->setCurrentHealth(497.f);
      stats->setMaxHealth(497.f);
      stats->setCurrentMana(255.f);
      stats->setMaxMana(255.f);
      stats->setBaseAd(46.6f);
      stats->setRange(550.f);
      stats->setMovementSpeed(325.f);
      stats->setArmor(16.7f);
      stats->setMagicArmor(30.f);
      stats->setHp5(5.6f);
      stats->setMp5(5.5f);
	    stats->setBaseSpeed(0.625f)
   }


};

#endif
